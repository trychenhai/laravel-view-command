{
  "name": "trychenhai/laravel-view-command",
  "description": "Easy to make view in Laravel with console.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "ChenhaiTry",
      "email": "chenhaitry64@gmail.com",
      "laravel/framework": "^5.4 || ^6.0 || ^7.0 || ^8.0"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5 || ^7.0 || ^8.0",
    "graham-campbell/testbench": "^4.0 || ^5.0",
    "mockery/mockery": "^1.4"
  },
  "support": {
    "issues": "https://github.com/trychenhai/laravel-view-command/issues",
    "source": "https://github.com/trychenhai/laravel-view-command"
  },
  "autoload": {
    "psr-4": {
      "Chenhaitry\\LaravelCommand\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": ["tests"]
  },
  "repositories": [
    {"type": "github", "url": "https://github.com/trychenhai/laravel-view-command"},
    {"packagist.org": false}
  ],
  "extra": {
    "laravel": {
      "providers": [
        "TryChenhai\\LaravelViewCommand\\Provider\\LaravelViewCommandServiceProvider"
      ]
    }
  }
}
